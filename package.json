{
    "name": "git-worktrees",
    "publisher": "GitWorktrees",
    "displayName": "Git Worktrees",
    "description": "Git Worktrees",
    "version": "0.0.2",
    "repository": {
        "type": "git",
        "url": "https://github.com/alexiszamanidis/vscode-git-worktrees"
    },
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:git-worktrees.worktree.list"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "git-worktrees.worktree.list",
                "title": "Git Worktrees: Git Worktree List"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "format": "yarn prettier --write ."
    },
    "devDependencies": {
        "@types/vscode": "^1.65.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "eslint": "^8.9.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.1",
        "typescript": "^4.5.5",
        "@vscode/test-electron": "^2.1.2",
        "prettier": "^2.5.1"
    }
}
